{
    "package": {
        "name": "samples",
        "description": "A plugin for viewing samples",
        "type": "iframe"
    },
    "services": {
        "route": {
            "routes": [
                {
                    "path": "samples/view/:sampleId/:-sampleVersion",
                    "view": "main",
                    "authorization": true
                },
                {
                    "path": "samples/about",
                    "view": "about",
                    "authorization": true
                }
            ]
        }
    }
}