                    "packing_type",
                    "ph",
                    "pore_water_extraction",
                    "recovery_factor",
                    "redox_potential_?",
                    "replicate",
                    "rock_formation",
                    "sampleid",
                    "screen_bottom_elevation",
                    "screen_end_depth",
                    "screen_start_depth",
                    "screen_top_elevation",
