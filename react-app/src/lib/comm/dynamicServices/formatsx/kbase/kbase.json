                    "nmdc:community",
                    "nmdc:description_long",
                    "nmdc:doi",
                    "nmdc:ecosystem",
                    "nmdc:ecosystem_category",
                    "nmdc:env_local_scale_term",
                    "nmdc:mod_date",
                    "nmdc:principal_investigator",
                    "nmdc:specific_ecosystem",
                    "nmdc:type",
                    "open",
                    "open_casing_depth",
