{
    "description": "A global format for KBase custom samples",
    "id": "kbase",
    "layouts": {
        "grouped": [
            {
                "description": "Fields which describe the overall sample event",
                "fields": [
                    "id",
                    "parent_id",
                    "igsn",
                    "parent_igsn",
                    "name",
                    "purpose",
                    "material",
                    "ENIGMA:material",
                    "SESAR:material",
                    "field_name",
                    "field_name_informal_classification",
                    "classification",
                    "field_program_cruise",
                    "sample_name",
                    "related_identifiers",
                    "relation_type",
                    "release_date",
                    "comment",
                    "other_names",
                    "related_url",
                    "related_url_type",
                    "related_url_description",
                    "sub_object_type",
                    "sample_id",
                    "description",
                    "condition",
                    "experiment_name"
                ],
                "label": "Description",
                "name": "description"
            },
            {
                "description": "Fields which describe the collection",
                "fields": [
                    "collection_date",
                    "collection_date_end",
                    "collection_time",
                    "collection_time_end",
                    "collection_date_precision",
                    "collector_chief_scientist",
                    "collector_chief_scientist_address",
                    "collection_method",
                    "collection_method_description",
                    "collection_time",
                    "date",
                    "time_zone",
                    "timezone"
                ],
                "label": "Collection",
                "name": "collection"
            },
            {
                "description": "Fields which describe the sample collection location",
                "fields": [
                    "coordinate_precision?",
                    "latitude",
                    "latitude_end",
                    "longitude",
                    "longitude_end",
                    "vertical_datum",
                    "northing",
                    "easting",
                    "navigation_type",
                    "locality_description",
                    "location_description",
                    "name_of_physiographic_feature",
                    "primary_physiographic_feature",
                    "elevation_start",
                    "elevation_end",
                    "elevation_unit",
                    "city_township",
                    "county",
                    "locality",
                    "state_province",
                    "country",
                    "continent",
                    "region",
                    "zone",
                    "geological_zone",
                    "area_name"
                ],
                "label": "Geolocation",
                "name": "geolocation"
            },
            {
                "description": "Sample measurements and observations",
                "fields": [
                    "sample_description",
                    "depth_in_core_max",
                    "depth_in_core_min",
                    "depth",
                    "depth_scale",
                    "launch_id",
                    "launch_platform_name",
                    "launch_type",
                    "platform_type",
                    "platform_description",
                    "platform_name",
                    "size",
                    "size_unit",
                    "age_min",
                    "age_max",
                    "age_unit",
                    "geological_age",
                    "geological_unit"
                ],
                "label": "Sample",
                "name": "sample"
            },
            {
                "description": "Environment Description and Measurements",
                "fields": [
                    "temperature",
                    "environmental_package",
                    "env_package"
                ],
                "label": "Environment",
                "name": "environment"
            },
            {
                "description": "Well Characteristics",
                "fields": [
                    "well_name",
                    "type_of_well",
                    "upper_seal_end_depth",
                    "upper_seal_start_depth",
                    "upper_seal_type",
                    "well",
                    "well_casing_depth",
                    "well_casing_id",
                    "well_casing_od",
                    "well_casing_type",
                    "well_status"
                ],
                "label": "Well",
                "name": "well"
            },
            {
                "description": "Fields which describe the curation of the sample",
                "fields": [
                    "current_archive",
                    "current_archive_contact",
                    "original_archive",
                    "original_archive_contact"
                ],
                "label": "Curation",
                "name": "curation"
            },
            {
