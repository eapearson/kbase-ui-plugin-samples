                    "top_of_casing_stickup",
                    "top_of_fresh_bedrock",
                    "top_of_weathered_bedrock",
                    "total_carbon",
                    "total_nitrogen",
                    "treatment"
                ],
                "label": "Uncategorized Fields",
                "name": "uncategorized"
            }
        ]
    },
    "mappings": {
        "record": {
            "name": "name"
        },
        "sample": {
            "id": "id",
